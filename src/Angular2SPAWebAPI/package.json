{
  "name": "angular2spawebapi",
  "version": "1.0.0",
  "description": "Angular 2 Single Page Application with an ASP.NET Core Web API that uses token authentication",
  "main": "",
  "scripts": {
    "ngc": "ngc -p ./tsconfig-aot.json",
    "webpack-dev": "set NODE_ENV=development&& webpack --watch",
    "webpack-prod": "set NODE_ENV=production&& webpack",
    "start": "npm run webpack-dev",
    "build": "npm run ngc && npm run webpack-prod"
  },
  "keywords": [
    "angular2",
    "typescript",
    "webapi",
    "aspnetcore",
    "identityserver4",
    "identity",
    "ROPC",
    "token authentication"
  ],
